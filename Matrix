#include<stdio.h>
int main()
{
    int A[10][10],B[10][10],C[10][10],i,j,p,q,m,n,k;
    printf("Enter the order of matrix A(p*q)");
    scanf("%d%d",&p,&q);
    printf("Enter the order of matrix B(m*n)");
    scanf("%d%d",&m,&n);
    if(q!=m)
    {
        printf("Matrix multiplication is not possible");
        return -1;
    }
    printf("Enter elements of matrix A:");
    for(i=0;i<p;i++)
    {
        for(j=0;j<q;j++)
        {
            scanf("%d",&A[i][j]);
        }
    }
    printf("Matrix A is:\n");
    for(i=0;i<p;i++)
    {
        for(j=0;j<q;j++)
        {
            printf("%d\t",A[i][j]);
        }
        printf("\n");
    }
    printf("Enter elements of matrix B:");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d",&B[i][j]);
        }
    }
    printf("Matrix B is:\n");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%d\t",B[i][j]);
        }
        printf("\n");
    }
    for(i=0;i<p;i++)
    {
        for(j=0;j<n;j++)
        {
            C[i][j]=0;
            for(k=0;k<q;k++)
            {
                C[i][j]+=A[i][k]*B[k][j];
            }
        }
    }
    printf("The resultant matrix C is:\n");
    for(i=0;i<p;i++)
    {
        for(j=0;j<q;j++)
        {
            printf("%d\t",C[i][j]);
        }
        printf("\n");
    }
    return 0;
}
